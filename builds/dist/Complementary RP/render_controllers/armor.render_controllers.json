{
	"format_version": "1.8.0",
	"render_controllers": {
		"controller.render.armor": {
			"geometry": "geometry.default",
			"materials": [
				{
					"*": "variable.is_enchanted?material.enchanted:material.default"
				}
			],
			"textures": [
				"variable.has_trim?variable.trim_path:texture.default",
				"texture.enchanted"
			],
			"is_hurt_color": {
				"r": 1,
				"g": 0,
				"b": 0,
				"a": 0.25
			},
			"on_fire_color": {
				"a": 0
			}
		}
	}
}